<div class="row">
    <div class="col-xs-12" id="experience">
        <h3>
            {{ 'core.resume.experience.title'|trans }}
        </h3>
        <hr class="visible-xs visible-sm">

        <p>
            <a href="http://www.udem37.fr/" title="&ldquo;{{ 'core.footer.projects.udem37.link_title'|trans }}&rdquo;" target="_blank" rel="noopener noreferrer">
                &ldquo;{{ 'core.footer.projects.udem37.title'|trans }}&rdquo;
            </a>
            <span class="right-pull">
                {{ 'core.resume.experience.udem37.date'|trans }}
            </span>
            <span class="normalWeight">
                {% for i in 0..1 %}
                    <span class="label label-success">
                        {{ ('core.projects.panel.fifth.technicals.' ~ i)|trans }}</span>
                    &nbsp;
                {% endfor %}
            </span>
        </p>
        <hr class="visible-xs visible-sm">

        <p>
            <a href="http://nao.fhuszti.tech/" title="&ldquo;{{ 'core.footer.projects.nao.link_title'|trans }}&rdquo;" target="_blank" rel="noopener noreferrer">
                &ldquo;{{ 'core.footer.projects.nao.title'|trans }}&rdquo;
